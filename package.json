{
  "name": "agency-hub",
  "version": "1.0.0",
  "description": "AI-powered client request management system for agencies",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "node --watch src/index.js",
    "dev:web": "vite",
    "dev:worker": "node --watch src/jobs/worker.js",
    "start": "node src/index.js",
    "build": "npx prisma generate && vite build",
